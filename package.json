{
  "name": "swag-landing-page",
  "type": "module",
  "private": true,
  "main": "index.js",
  "scripts": {
    "build": "npm run pages:build && npm run styles:build && npm run pages:build",
    "dev": "npm run styles:build && npm run pages:dev & npm run styles:watch",
    "serve": "http-server -p 3000 dist/",
    "pages:build": "elm-pages build",
    "pages:dev": "elm-pages develop",
    "styles:build": "npx postcss css/style.css -o css/built.css --config css/postcss.config.js",
    "styles:watch": "npx postcss css/style.css -o css/built.css --config css/postcss.config.js --watch"
  },
  "devDependencies": {
    "@tailwindcss/ui": "^0.5.0",
    "elm": "^0.19.1-3",
    "elm-pages": "^1.4.2",
    "elm-tailwind-css": "^0.4.6",
    "fission-kit": "github:fission-suite/kit#52007072cf459d02de6b49f5ca13229a7a03f4c6",
    "http-server": "^0.12.3",
    "postcss-cli": "^7.1.1",
    "tailwindcss": "^1.6.2"
  }
}
